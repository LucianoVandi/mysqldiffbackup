{
    "name": "lvandi/mysqldiffbackup",
    "type": "library",
    "description": "Mysql differential backup",
    "keywords": [],
    "homepage": "https://github.com/lucianovandi/MysqlDiffBackup",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano",
            "email": "vandi.luciano@gmail.com",
            "homepage": "https://github.com/lucianovandi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "monolog/monolog": "~2.0",
        "psr/log": "^1.1",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Lvandi\\MysqlDiffBackup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lvandi\\MysqlDiffBackup\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }, 
    "bin": [
        "bin/dumper"
    ]
}
